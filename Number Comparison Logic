#include <stdio.h>

#define RIGHT 555

void guessNumber(int guess) {
    if (guess < RIGHT) {
        printf("Your guess is too low.\n");
    } else if (guess > RIGHT) {
        printf("Your guess is too high.\n");
    } else {
        printf("You guessed it!\n");
    }
}

int main() {
    guessNumber(500);
    guessNumber(600);
    guessNumber(555);
    
    return 0;
}
